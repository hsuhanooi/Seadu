$('#questions').attr('data-page', '<%= @page %>')
<% @questions.each do |question| %>
  $("#question_list").prepend("<li><%= escape_javascript( 
      render :partial => 'questions/question', :locals => {:question => question}
    ) %></li>");
    if ($('#questions li').length > 10) {
      $('#questions li:last').remove();
    };
<% end %>