<%= javascript_include_tag 'highcharts' %>

<%= Vibe.chart_series(@room.id) %>
<br>
LISTENERS: <%= @room.num_listeners %>
<br>
<%= link_to "Randomize", randomize_chart_url(@room.id) %>
<br>
<%= link_to "Bored", create_vibe_url(:room_id => @room.id, :vibe_type => :bored), :remote => true %>
<br>
<%= link_to "Good", create_vibe_url(:room_id => @room.id, :vibe_type => :good), :remote => true %>
<br>
<%= link_to "Confused", create_vibe_url(:room_id => @room.id, :vibe_type => :confused), :remote => true %>
<br>
<%= link_to "Close Room", finish_room_url(:room_id => @room.id), :remote => true %>

<%= render_vibe_chart(@room) %>
